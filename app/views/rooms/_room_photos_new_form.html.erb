<%= form_with(model: @photo, local: true) do |f| %>

  <%= f.hidden_field :room_id, value: @room.id %>

  <div class="form-group">
    <%= f.label :url %>

    <%= f.file_field :url, class: "form-control" %>
  </div>

  <%= f.submit "Create photo", class: "btn btn-block btn-outline-secondary" %>
<% end %>
